{
  "name": "@nandenjin/alien-ui",
  "version": "0.1.3",
  "description": "UI Components Kit written with Vue.js",
  "main": "dist/alien-ui.umd.js",
  "module": "dist/alien-ui.esm.js",
  "unpkg": "dist/alien-ui.min.js",
  "repository": "https://github.com/nandenjin/alien-ui.git",
  "author": "Kazumi Inada <hello@nandenjin.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-p build:*",
    "build:umd": "rollup --config rollup.config.mjs --format umd --file dist/alien-ui.umd.js",
    "build:es": "rollup --config rollup.config.mjs --format es --file dist/alien-ui.esm.js",
    "build:unpkg": "rollup --config rollup.config.mjs --format iife --file dist/alien-ui.min.js",
    "lint": "eslint src/**/*"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^2.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.2",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue-property-decorator": "^9.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
